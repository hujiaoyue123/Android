<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="75px">

    <ImageView
        android:id="@+id/openclass_live_list_icon"
        android:layout_width="30px"
        android:layout_height="30px"
        android:layout_marginLeft="28px"
        android:layout_marginTop="22px"
        android:layout_centerVertical="true"
        android:layout_marginBottom="33px"
        app:srcCompat="@mipmap/video" />

    <TextView
        android:id="@+id/openclass_live_list_title"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/openclass_live_list_icon"
        android:layout_marginTop="22px"
        android:layout_marginLeft="42px"
        android:layout_marginBottom="33px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/openclass_live_list_to_icon"
        android:layout_width="30px"
        android:layout_height="30px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="42px"
        android:layout_marginBottom="33px"
        android:layout_marginTop="22px"
        app:srcCompat="@mipmap/to"
        android:layout_alignRight="@+id/openclass_live_list_title"/>

</com.zhy.autolayout.AutoRelativeLayout>